<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

    <context:component-scan base-package="com.cib.dubbo.consumer.controller"/>

    <!--声明服务消费着的名称，也要保证名称的保证唯一性-->
    <dubbo:application name="005-registry-dubbo-consumerr"/>

    <dubbo:registry protocol="zookeeper" address="192.168.8.105:2181"/>

    <dubbo:reference id="userService" interface="com.cib.dubbo.service.UserService"/>

<!--    &lt;!&ndash;dubbo注解所在的包信息&ndash;&gt;-->
<!--    <dubbo:annotation package="com.goods.display.controller"/>-->

    
</beans>